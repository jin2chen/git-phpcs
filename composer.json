{
    "name": "mole-starlight/git-phpcs",
    "description": "Git hook for PHP code style.",
    "type": "library",
    "keywords": ["php", "code style", "phpcs"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mole-starlight/git-phpcs/issues",
        "source": "https://github.com/mole-starlight/git-phpcs"
    },
    "authors": [
        {
            "name": "Jin Chen",
            "email": "jin.chen@jinchen.me"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "mole\\php\\cs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mole\\php\\cs\\test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
